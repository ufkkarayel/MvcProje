@model IEnumerable<EntityLayer.Concrete.Category>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h2> Burası Index</h2>   
        <br />
        <table>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Kategori Adı
                </th>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@item.CategoryId</td>
                    <td>
                        @item.CategoryName
                    </td>
                </tr>


                }
            </tr>
        </table>
    </div>
</body>
</html>
